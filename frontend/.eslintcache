    <input type='text' required id='duration' onChange={this.onChange} />\n                    <div className=\"text-danger\">{this.state.errors.duration}</div>\n                </div>\n\n                {\n                    <div>\n                        <label>skills required</label>\n                        <Creatable onChange={(value) => this.onChangeSkills(value)}\n                            isMulti\n                            options={defaultLangs}\n                            value={this.state.skills}\n                        />\n                        <div className=\"text-danger\">{this.state.errors.skills}</div>\n\n                    </div>\n                }\n\n                <button onClick={() => { this.onSubmit() }} >add job</button>\n\n                <div style={{ marginTop: 20 }}>{JSON.stringify(this.state.jobData)}</div>\n                <div style={{ marginTop: 20 }}>{JSON.stringify(this.state.errors)}</div>\n                <br/>\n                {JSON.stringify(this.state.skills)}\n\n            </div>\n        )\n    }\n}","/Users/saru/dass/ass1/ytTut/frontend/src/components/recruiter/recViewAcc.jsx",["157","158","159"],"import axios from 'axios';\nimport { Col, Row } from 'react-bootstrap';\nimport React, { Component } from 'react';\nimport RecAppCard from './RecAppCard'\n\nexport default class recViewAcc extends Component {\n\n    constructor(props) {\n        super(props);\n        this.state = {\n            Loading: true,\n            applicationList: null,\n        }\n        this.onSortRating = this.onSortRating.bind(this);\n        this.onSortName = this.onSortName.bind(this);\n        this.onSortDate = this.onSortDate.bind(this);\n    }\n\n    componentDidMount() {\n        axios.post('http://localhost:5000/recruiter/getAccForRec')\n            .then(res => {\n                this.setState({\n                    applicationList: res.data,\n                    Loading: false\n                });\n\n            }).catch(err => { c